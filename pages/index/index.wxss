/* 一件加油蒙版 */

@keyframes animation {
  from {
    opacity: 0;
    -webkit-transform: scale(1.2) rotateX(45deg);
    transform: scale(1.2) rotateX(45deg);
  }

  to {
  }
}

.onekey_mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 99;
  overflow: hidden;
  animation: animation ease 1s;
  animation-delay: 0.8s;
  animation-fill-mode: backwards;
  perspective: 1000px;
  -webkit-perspective: 1000px;
  transition: ease-in-out 0.2s;
}
.pop_box{
  width: 100%;
  height: 100%;
  position: relative;
}

.pop_box_item {
  position: absolute;
  left: 0;
  right: 0;
  margin: auto;
  width: 80%;
  height: 760rpx;
  background-color: #fff;
  text-align: center;
  border-radius: 8rpx;
  margin-top: 20%;
  z-index: 999;
}

