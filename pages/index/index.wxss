}

view.tab_select_motorcade_private {
  text-align: right;
  padding-right: 30rpx;
  line-height: 88rpx;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

view.tab_select_motorcade_private>button {
  display: inline-block;
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  border: 1px solid #f93;
  margin: 0;
  padding: 0;
}

view.tab_select_motorcade_private>button.button-hover {
  background-color: #f93;
  color: #fff;
}

view.tab_select_motorcade_private>button:active {
  background-color: #f93;
  color: #fff;
}

view.tab_select_motorcade_private>button::after {
  background-color: transparent;
  color: #f93;
}

view.tab_select_motorcade_private>button:first-child {
  border-top-right-radius: 0;
  border-bottom-right-radius: 0;
  border-right: none;
}

view.tab_select_motorcade_private>button:last-child {
  border-top-left-radius: 0;
  border-bottom-left-radius: 0;
}

view.tab_select_motorcade_private>button.grey {
  background-color: #fff;
  color: #f93;
}

