/* pages/identity_registration/identity_success/index.wxss */

.not_support_identity {
  text-align: center;
}

image {
  width: 84px;
  height: 84px;
  margin-top: 30%;
}

.error_title {
  width: 200px;
  font-size: 20px;
  color: #222;
  letter-spacing: 0;
  text-align: center;
  line-height: 28px;
  margin: 0 auto;
  margin-top: 30px;
  font-weight: bold;
}

.error_reason {
  font-family: PingFangSC-Regular;
  font-size: 12px;
  color: rgba(0, 0, 0, 0.54);
  letter-spacing: 0;
  text-align: center;
  line-height: 16px;
  width: 250px;
  margin: 20px auto;
}

view.ikonw {
  width: 142px;
  height: 40px;
  border: 1px solid #f93;
  line-height: 50px;
  font-size: 17px;
  color: #ff7a21;
  border-radius: 5px;
  text-align: center;
  line-height: 40px;
  margin: 20px auto;
  margin-top: 100px;
}

.ikonw_else {
  font-size: 12px;
  color: rgba(0, 0, 0, 0.54);
  position: absolute;
  bottom: 20px;
  text-align: center;
  margin: auto;
  width: 100%;
}

.iprivate {
  line-height: 60px;
  font-size: 17px;
  color: #ff7a21;
}

.ioperating_car {
  line-height: 60px;
  font-size: 17px;
  color: #ff7a21;
}
